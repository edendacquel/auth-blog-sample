{
    "routes": [
        {
            "route": "/login",
            "rewrite": "/.auth/login/aad"
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        },
        {
            "route":"/Test/*",
            "allowerRoles": ["reader"]
        }
    ],
    "platformErrorOverrides": [
        {
          "errorType": "NotFound",
          "serve": "/custom-404.html"
        },
        {
          "errorType": "Unauthenticated",
          "serve": "/login"
        }
      ]
}